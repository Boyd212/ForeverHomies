{
  "name": "foreverhomies",
  "jest": {
    "setupFilesAfterEnv": [
      "./src/setupTests.js"
    ],
    "transform": {
      "\\.(js|jsx)$": [
        "babel-jest"
      ]
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs)$"
    ],
    "verbose": true
  },
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/user-event": "14.4.3",
    "bootstrap": "5.3.1",
    "node-sass": "9.0.0",
    "prop-types": "^15.8.1",
    "react": "18.2.0",
    "react-bootstrap": "2.8.0",
    "react-dom": "18.2.0",
    "react-redux": "8.1.2",
    "react-router-dom": "6.14.2",
    "react-scripts": "5.0.1",
    "redux": "4.2.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "jest",
    "test:watch": "npm test --watch",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "responsive",
      "not op_mini all"
    ],
